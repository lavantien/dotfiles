  - deepseek=
  - codestral=
  - mistral=
  - github=
  - cohere=
  - xai=
  - groq=
  - together=
  - openrouter=
architect: true
model: deepseek/deepseek-reasoner
# model: o3-mini
# model: o1
# model: anthropic/claude-3-7-sonnet-20250219
# model: gemini/gemini-2.0-pro-exp-02-05
# model: gemini/gemini-2.0-flash-thinking-exp-01-21
# edit-format: diff
editor-model: deepseek/deepseek-chat
# model: anthropic/claude-3-7-sonnet-20250219
# editor-model: gpt-4o
# editor-model: gemini/gemini-2.0-flash
# editor-model: gemini/gemini-2.0-flash-lite
# editor-model: codestral/codestral-latest
# editor-model: mistral/mistral-large-latest
# editor-edit-format: editor-diff
weak-model: codestral/codestral-latest
# weak-model: gpt-4o-mini
# weak-model: mistral/mistral-large-latest
# weak-model: mistral/pixtral-large-latest
read:
# - SYSTEM_PROMPT_PROGRAMMING.xml
# - PRELIMINARY_DESIGN.md
yes-always: false
auto-commits: true
attribute-author: false
attribute-committer: false
show-diffs: false
# test-cmd: go test ./... -v -race -cover -timeout 10s
# auto-test: true
map-tokens: 2048
map-multiplier-no-files: 4
subtree-only: true
timeout: 301
cache-prompts: true
cache-keepalive-pings: 300
verbose: false
pretty: true
vim: true
editor: vim
multiline: true
detect-urls: false
code-theme: coffee
